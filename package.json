{
  "name": "cassette",
  "version": "0.0.0",
  "description": "Cassette is a modern music database manager and grabber buit on NodeJS",
  "main": "server.js",
  "scripts": {
    "test": "ava -v",
    "test-watch": "ava --watch",
    "start": "babel-node src/server/server.js --presets es2015,es2016,stage-2 --dev",
    "coverage": "cross-env NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage-travis": "cross-env NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "vinz243",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "cross-env": "^3.1.3",
    "file-exists": "^2.0.0",
    "koa": "^2.0.0",
    "koa-better-body": "^3.0.0",
    "koa-router": "^7.0.1",
    "lazy.js": "^0.4.2",
    "mkdirp": "^0.5.1",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.0",
    "pascal-case": "^2.0.0",
    "pluralize": "^3.0.0",
    "shortid": "^2.2.6",
    "snake-case": "^2.1.0",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "chokidar": "^1.6.1",
    "coveralls": "^2.11.14",
    "nock": "^8.0.0",
    "nyc": "^8.3.2",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0",
    "supertest-koa-agent": "^0.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "stage-2"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
