{
  "name": "cassette",
  "version": "0.0.0",
  "description": "Cassette is a modern music database manager and grabber buit on NodeJS",
  "main": "server.js",
  "scripts": {
    "ava": "ava",
    "clean:build": "rimraf build lib",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "npm run clean:build && npm run backend:build ",
    "start": "webpack-dashboard -c cyan -- node dev-server.js",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": "npm run build:html",
    "build:serve": "http-server build/client -p 3003 --cors -o",
    "lint": "eslint config src/js/** --ext .js",
    "jscs": "jscs src/js/",
    "typecheck": "flow",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch",
    "tunnel": "lt --port 3000",
    "backend:test": "npm run backend:build && ava lib/server/**/*.test.js -v",
    "test-watch": "ava src/server  --watch",
    "backend:build": "babel src/server --presets es2015,es2016,stage-2 --out-dir lib/server --ignore *.test.js",
    "backend:start": "babel-node src/server/server.js --presets es2015,es2016,stage-2 --dev",
    "backend:coverage": "cross-env NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "backend:coverage-travis": "cross-env NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "vinz243",
  "license": "MIT",
  "dependencies": {
    "antd": "2.5.2",
    "autoprefixer": "6.4.1",
    "axios": "0.15.3",
    "babel-polyfill": "6.20.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.11.6",
    "classnames": "2.2.5",
    "cross-env": "^3.1.3",
    "express": "4.14.0",
    "file-exists": "^2.0.0",
    "howler": "2.0.2",
    "koa": "^2.0.0",
    "koa-better-body": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.1",
    "lazy.js": "^0.4.2",
    "lodash": "4.15.0",
    "mediastic": "1.1.3",
    "mkdirp": "^0.5.1",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.0",
    "pascal-case": "^2.0.0",
    "pluralize": "^3.0.0",
    "raven-js": "3.6.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-hot-loader": "3.0.0-beta.2",
    "react-icons": "2.2.2",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "scanpel": "^0.3.2",
    "shortid": "^2.2.6",
    "snake-case": "^2.1.0",
    "underscore.string": "3.3.4"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-import": "1.1.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-2": "^6.16.0",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "cheerio": "0.22.0",
    "chokidar": "^1.6.1",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "coveralls": "^2.11.14",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "delegates": "1.0.0",
    "eslint": "3.4.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-react": "6.2.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "flow-bin": "0.31.1",
    "http-server": "0.9.0",
    "jquery": "3.1.0",
    "jscs": "3.0.7",
    "jsdom": "9.4.5",
    "mocha": "3.0.2",
    "nock": "^8.0.0",
    "node-sass": "3.13.1",
    "nyc": "^8.3.2",
    "postcss-loader": "0.11.1",
    "react-addons-test-utils": "15.3.1",
    "redbox-react": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-slider-monitor": "1.0.7",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.1",
    "style-loader": "0.13.1",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0",
    "supertest-koa-agent": "^0.3.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.21",
    "webpack-dashboard": "0.1.8",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-merge": "0.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "babel": {
      "babelrc": false,
      "presets": [
        "es2015",
        "es2016",
        "stage-0"
      ],
      "env": {
        "test": {
          "plugins": [
            "istanbul"
          ]
        }
      }
    },
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
